{

    "manifest_version": 2,
    "name": "tabmap",
    "version": "1.0",
  
    "description": "Silly prototype",
  
    "icons": {
      "48": "icons/uml.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.wikipedia.org/*"],
        "js": ["tabmap.js"]
      }
    ],
    "permissions": [
      "<all_urls>",
      "tabs"
    ],
    "browser_action": {
      "browser_style": true,
      "default_title": "Wikipedia to Wikidata board",
      "default_popup": "popup/conceptboard.html"
    }
  
  }
  